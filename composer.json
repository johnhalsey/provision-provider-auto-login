{
    "name": "upmind/provision-provider-auto-login",
    "description": "Basic provisioning category/provider for obtaining signed urls to enable auto login to compatible online services",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Harry Lewis",
            "email": "harry@upmind.com"
        }
    ],
    "require": {
        "php": "^7.4 || 8.1 - 8.3",
        "upmind/provision-provider-base": "^2.0 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Upmind\\ProvisionProviders\\AutoLogin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Upmind\\ProvisionProviders\\AutoLogin\\LaravelServiceProvider"
            ]
        }
    }
}
